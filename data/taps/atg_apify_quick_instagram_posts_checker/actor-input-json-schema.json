{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "startUrls": {
            "type": "array",
            "description": "Only URLs to public profiles or posts expected to work, everything else will return error",
            "example": [
                {
                    "url": "https://www.instagram.com/kimkardashian"
                },
                {
                    "url": "https://www.instagram.com/p/CrzMaNlP_3G"
                }
            ]
        },
        "proxy": {
            "type": "object",
            "description": "Either use an Apify residential proxy, or provide your own proxy servers. Datacenter proxies will not work.",
            "example": {
                "useApifyProxy": true,
                "apifyProxyGroups": [
                    "RESIDENTIAL"
                ]
            }
        }
    },
    "required": [
        "startUrls"
    ]
}