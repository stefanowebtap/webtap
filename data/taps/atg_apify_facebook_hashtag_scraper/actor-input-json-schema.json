{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "keywordList": {
            "type": "array",
            "description": "Spaces or special characters will bring no results",
            "example": [
                "webscraping"
            ]
        },
        "resultsLimit": {
            "type": "integer",
            "description": "Max amount of results per keyword",
            "example": 50
        }
    },
    "required": [
        "keywordList"
    ]
}