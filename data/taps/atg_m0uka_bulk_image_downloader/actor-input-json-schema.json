{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "startUrls": {
            "type": "array",
            "description": "URLs to start with.",
            "example": [
                {
                    "url": "https://apify.com"
                }
            ]
        },
        "proxy": {
            "type": "object",
            "description": "Select proxies to be used by the scraper.",
            "example": {
                "useApifyProxy": true
            }
        }
    },
    "required": [
        "startUrls"
    ]
}