{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "url": {
            "type": "string",
            "description": "Url to check",
            "example": "https://mall.cz"
        },
        "proxy": {
            "type": "object",
            "description": "Select proxies to be used by your crawler.",
            "example": {
                "useApifyProxy": true
            }
        }
    },
    "required": []
}