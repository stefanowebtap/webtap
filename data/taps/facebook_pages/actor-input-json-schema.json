{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "startUrls": {
        "type": "array",
        "description": "An array of objects, each containing a URL of a Facebook page to scrape. These URLs must be for Facebook pages, not personal profiles.",
        "items": {
          "type": "object",
          "properties": {
            "url": {
              "type": "string",
              "format": "uri",
              "description": "The URL of the Facebook page you want to scrape."
            }
          },
          "required": ["url"],
          "example": {
            "url": "https://www.facebook.com/copperkettleyqr/"
          }
        }
      }
    },
    "required": ["startUrls"],
    "example": {
      "startUrls": [
        {
          "url": "https://www.facebook.com/copperkettleyqr/"
        },
        {
          "url": "https://www.facebook.com/HiltonGardenInnMontrealAirport/"
        }
      ]
    }
  }
  